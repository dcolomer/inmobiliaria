#Fase de desarrollolog4j.rootLogger=DEBUG, PANTALLA#Fase de produccion#log4j.rootLogger=WARN, PANTALLAlog4j.appender.PANTALLA=org.apache.log4j.ConsoleAppenderlog4j.appender.PANTALLA.layout=org.apache.log4j.PatternLayoutlog4j.appender.PANTALLA.layout.ConversionPattern=%d [%t] %-5p %c - %m%n
